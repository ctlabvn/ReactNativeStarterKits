{
  "name": "ReactNativeStarterKits",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start -- --reset-cache",
    "keyiOS": "code-push deployment ls reactnativestarterkits-ios -k",
    "keyAndroid": "code-push deployment ls reactnativestarterkits-android -k",
    "releaseiOS": "code-push release-react reactnativestarterkits-ios ios --description \"Optional release description iOS here\"",
    "releaseAndroid": "code-push release-react reactnativestarterkits-android android --description \"Optional release description Android here\"",
    "releaseForceiOS": "code-push release-react reactnativestarterkits-ios ios -m --description \"Mandatory release description iOS here\"",
    "releaseForceAndroid": "code-push release-react reactnativestarterkits-android android -m --description \"Mandatory release description Android here\"",
    "promoteiOS": "code-push promote reactnativestarterkits-ios Staging Production",
    "promoteAndroid": "code-push promote reactnativestarterkits-android Staging Production",
    "rollbackStagingiOS": "code-push rollback reactnativestarterkits-ios Staging",
    "rollbackStagingAndroid": "code-push rollback reactnativestarterkits-android Staging",
    "rollbackProiOS": "code-push rollback reactnativestarterkits-ios Production",
    "rollbackProAndroid": "code-push rollback reactnativestarterkits-android Production",
    "statisticStagingiOS": "code-push deployment history reactnativestarterkits-ios Staging",
    "statisticStagingAndroid": "code-push deployment history reactnativestarterkits-android Staging",
    "statisticProiOS": "code-push deployment history reactnativestarterkits-ios Production",
    "statisticProAndroid": "code-push deployment history reactnativestarterkits-android Production",
    "test": "jest"
  },
  "dependencies": {
    "add": "^2.0.6",
    "apisauce": "^0.14.1",
    "color": "^2.0.0",
    "lodash": "^4.17.4",
    "native-base": "^2.3.1",
    "prop-types": "^15.5.10",
    "react": "16.0.0-beta.5",
    "react-native": "0.49.3",
    "react-native-fast-image": "^1.0.0",
    "react-native-image-picker": "^0.26.6",
    "react-native-maps": "^0.16.2",
    "react-native-tab-view": "^0.0.69",
    "react-native-vector-icons": "^4.3.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-form": "^7.0.3",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.9.1",
    "redux-saga": "^0.15.6",
    "yarn": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "20.0.3",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-remove-console": "^6.8.5",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-native": "3.0.1",
    "eslint-config-rallycoding": "^3.2.0",
    "flow-bin": "0.49.1",
    "jest": "20.0.4",
    "react-test-renderer": "16.0.0-alpha.12"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts"
    ]
  },
  "jest": {
    "preset": "react-native"
  }
}
